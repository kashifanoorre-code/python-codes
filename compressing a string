l1=[]
n=int(input("enter number of elements"))
for i in range(n):
    element=(input("enter element"))
    l1.append(element)
print(l1)

result=""
count=1
for i in range(len(l1)-1):
    if l1[i]==l1[i+1]:
        count+=1
    else:
        result= result+ l1[i]+ str(count)
        count=1

result=result+l1[-1]+str(count)
print(result)
