matrix=[
    [0,0,0],
    [0,0,0],
    [0,0,0]
    ]
rows=3
columns=3
for i in range(rows):
    for j in range(columns):
        matrix[i][j]=int(input('enter element'))
        

for rows in matrix:
    print(rows)

sum_of_diagonal_elements=0
for i in range(3):
    for j in range(3):
        if i==j:
            sum_of_diagonal_elements+=matrix[i][j]
        else:
            continue
print(sum_of_diagonal_elements)
