x=[
    [1,2,3],
    [2,4,1],
    [2,4,9]
]

y=[
    [1,0,0],
    [0,1,0],
    [0,0,1]
    ]
result = [
    [0, 0, 0],
    [0, 0, 0],
    [0, 0, 0]
]
    # adding two matrices
for i in range(len(x)):
    for j in range(len(x[0])):
        result[i][j] = x[i][j]+ y[i][j]
for r in result:
    print(r)
