lst = list(map(int, input("Enter numbers: ").split()))

n = len(lst)
found = False

for i in range(n):
    current_sum = 0
    for j in range(i, n):
        current_sum += lst[j]
        if current_sum == 0:
            print(lst[i:j+1])
            found = True

if not found:
    print("No sublist with sum 0 found")
